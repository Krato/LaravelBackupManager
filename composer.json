{
    "name": "infinety-es/laravelBackupManager",
    "description": "Laravel 5 admin interface for manage backups.",
    "keywords": [
        "backup",
        "backupmanager",
        "infinety",
        "laravel"
    ],
    "homepage": "https://github.com/krato/LaravelBackupManager",
    "license": "MIT",
    "authors": [
        {
            "name": "Infinety",
            "email": "hello@infinety.es"
        }
    ],
    "require": {
        "illuminate/support": "~5.1",
        "php" : ">=5.3.0",
        "spatie/laravel-backup": "^2.4"
    },
    "require-dev": {
        "phpunit/phpunit" : "4.*",
        "scrutinizer/ocular": "~1.1"
    },
    "autoload": {
        "psr-4": {
            "Infinety\\BackupManager\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Infinety\\BackupManager\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
